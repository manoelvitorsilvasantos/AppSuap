<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/white">

	<WebView
		android:id="@+id/webView"
		android:layout_width="match_parent"
		android:layout_height="match_parent"/>

	<ProgressBar
		android:id="@+id/progress"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center"/>

	<android.support.design.widget.FloatingActionButton
		android:id="@+id/fabOne"
		android:layout_width="@dimen/fabHW"
		android:layout_height="@dimen/fabHW"
		android:layout_alignParentBottom="true"
		android:layout_alignParentEnd="true"
		android:layout_marginBottom="@dimen/marginFabs"
		androidylayout_marginEnd="@dimen/marginFabs"
		android:src="@drawable/ifba_icon"/>

	<android.support.design.widget.FloatingActionButton
		android:id="@+id/fabTwo"
		android:layout_width="@dimen/fabHW"
		android:layout_height="@dimen/fabHW"
		android:layout_alignParentBottom="true"
		android:layout_alignParentEnd="true"
		android:layout_marginBottom="@dimen/marginFabs"
		androidylayout_marginEnd="@dimen/marginFabs"
		android:src="@drawable/ifba_icon"/>

	<android.support.design.widget.FloatingActionButton
		android:id="@+id/fabThree"
		android:layout_width="@dimen/fabHW"
		android:layout_height="@dimen/fabHW"
		android:layout_alignParentBottom="true"
		android:layout_alignParentEnd="true"
		android:layout_marginBottom="@dimen/marginFabs"
		androidylayout_marginEnd="@dimen/marginFabs"
		android:src="@drawable/ifba_icon"/>

	<android.support.design.widget.FloatingActionButton
		android:id="@+id/fab"
		android:layout_width="@dimen/fabHW"
		android:layout_height="@dimen/fabHW"
		android:layout_alignParentBottom="true"
		android:layout_alignParentEnd="true"
		android:layout_marginBottom="@dimen/marginFabs"
		androidylayout_marginEnd="@dimen/marginFabs"
		android:src="@drawable/ifba_icon"/>

</RelativeLayout>

